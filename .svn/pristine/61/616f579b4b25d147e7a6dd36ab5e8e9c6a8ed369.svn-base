/**
 * 업무확인 페이지의 공통 Function
 */

$(document).ready(function() {

	// 첨부파일
	$("#atchFileId").on('change', function() {
		$(this).next('.custom-file-label').html(event.target.files[0].name);
	});

	$('#startDt').datetimepicker({
		format : 'YYYY-MM-DD',
		/* format: 'L', */
		useCurrent : true
	});

	$('#endDt').datetimepicker({
		format : 'YYYY-MM-DD',
		/* format: 'L', */
		useCurrent : true
	});
	

});
