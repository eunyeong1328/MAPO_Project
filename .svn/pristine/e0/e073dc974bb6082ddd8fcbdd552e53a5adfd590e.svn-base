/* 메인화면 좌측 로고 Text */
p.mainText {
	/* color:	#148EFF; */
	color: #FFFFFF;
	font-size: 1.3em;
	font-family: Roboto; 
	/*  font-family: MapoAgape'; */
	margin-top: 12px;
}

/* 화면 타이틀 */
p.pageTitle {
	color:	#111A62;
	font-size: 1.5em;
	font-family: MapoDPP;
	/* font-weight: bold; */
}

p.pageTitle i {
	color : #51545E;
	margin-right: 4px;
	margin-left: 6px;
	margin-top: 4px;
	vertical-align: center;
	
}

/* font 컬러 검정색 */
